{
    "exclude": "**/node_modules/**/*",
    "extends": [
        "tslint:latest",
        "tslint-react",
        "tslint-eslint-rules",
        "tslint-config-prettier"
    ],
    "rules": {
        "interface-name": [true, "never-prefix"],
        "no-bitwise": false,
        "object-literal-sort-keys": false,
        "variable-name": [true, "ban-keywords", "check-format", "allow-snake-case"]
    }
}
